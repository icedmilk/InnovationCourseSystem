<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name></display-name>
  
  <listener>
    <listener-class>smu.vccs.web.listener.SessionCounter</listener-class>
  </listener>
  
  <servlet>
    <servlet-name>RegisterServlet</servlet-name>
    <servlet-class>smu.vccs.web.servlet.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>smu.vccs.web.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ShowCourseServlet</servlet-name>
    <servlet-class>smu.vccs.web.servlet.ShowCourseServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>WhatIChoose</servlet-name>
    <servlet-class>smu.vccs.web.servlet.WhatIChoose</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>MessageBox</servlet-name>
    <servlet-class>smu.vccs.web.servlet.MessageBox</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>OverallSituation</servlet-name>
    <servlet-class>smu.vccs.web.servlet.OverallSituation</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ChooseCourse</servlet-name>
    <servlet-class>smu.vccs.web.servlet.ChooseCourse</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AddCourse</servlet-name>
    <servlet-class>smu.vccs.web.servlet.AddCourse</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DeleteCourse</servlet-name>
    <servlet-class>smu.vccs.web.servlet.DeleteCourse</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CancelChoose</servlet-name>
    <servlet-class>smu.vccs.web.servlet.CancelChoose</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CoreAlgorithm</servlet-name>
    <servlet-class>smu.vccs.web.servlet.CoreAlgorithm</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GetStatus</servlet-name>
    <servlet-class>smu.vccs.web.servlet.GetStatus</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SuggestAddCourse</servlet-name>
    <servlet-class>smu.vccs.web.servlet.SuggestAddCourse</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CourseNumberValidate</servlet-name>
    <servlet-class>smu.vccs.web.servlet.CourseNumberValidate</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Validate</servlet-name>
    <servlet-class>smu.vccs.web.servlet.Validate</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>JudgeValidate</servlet-name>
    <servlet-class>smu.vccs.web.servlet.JudgeValidate</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GetSessionCount</servlet-name>
    <servlet-class>smu.vccs.web.servlet.GetSessionCount</servlet-class>
  </servlet>











  <servlet-mapping>
    <servlet-name>RegisterServlet</servlet-name>
    <url-pattern>/register.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ShowCourseServlet</servlet-name>
    <url-pattern>/CourseInfo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>WhatIChoose</servlet-name>
    <url-pattern>/ChosenCourse.show</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MessageBox</servlet-name>
    <url-pattern>/MessageBox.show</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>OverallSituation</servlet-name>
    <url-pattern>/OverallSituation.show</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ChooseCourse</servlet-name>
    <url-pattern>/ChooseCourse.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddCourse</servlet-name>
    <url-pattern>/AddCourse.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeleteCourse</servlet-name>
    <url-pattern>/DeleteCourse.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CancelChoose</servlet-name>
    <url-pattern>/CancelChoose.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CoreAlgorithm</servlet-name>
    <url-pattern>/CoreAlgorithm.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetStatus</servlet-name>
    <url-pattern>/GetStatus.show</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SuggestAddCourse</servlet-name>
    <url-pattern>/SuggestAddCourse.show</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CourseNumberValidate</servlet-name>
    <url-pattern>/CourseNumberValidate.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Validate</servlet-name>
    <url-pattern>/imageServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>JudgeValidate</servlet-name>
    <url-pattern>/JudgeValidate.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetSessionCount</servlet-name>
    <url-pattern>/GetSessionCount</url-pattern>
  </servlet-mapping>



  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>
